$ORIGIN {{VPC_SHORT_NAME}}.compute.local.
$TTL 86400  ; 1 day
@                   SOA {{VPC_SHORT_NAME}}-dns root (
    0          ; serial
    3600       ; refresh (1 hour)
    3600       ; retry (1 hour)
    604800     ; expire (1 week)
    10800      ; minimum (3 hours)
)
                    NS  {{VPC_SHORT_NAME}}-dns
; TODO: The third and fourth octet of the DNS server's IP address needs to be updated here
{{VPC_SHORT_NAME}}-dns    A   10.{{SECOND_OCTET}}.0.1
