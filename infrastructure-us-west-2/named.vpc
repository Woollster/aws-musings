$ORIGIN {{DNS_ZONE}}.compute.local.
$TTL 86400  ; 1 day
@                   SOA {{DNS_ZONE}}-dns root (
    0          ; serial
    3600       ; refresh (1 hour)
    3600       ; retry (1 hour)
    604800     ; expire (1 week)
    10800      ; minimum (3 hours)
)
                    NS  {{DNS_ZONE}}-dns
; TODO: The third and fourth octet of the DNS server's IP address needs to be updated here
{{DNS_ZONE}}-dns    A   10.{{SECOND_OCTET}}.0.1
